   "source": [
    "class Solution:\n",
    "    def thirdMax(self, nums: List[int]) -> int:\n",
    "        lst=list(set(nums))\n",
    "        lst.sort()\n",
    "        if len(lst)<=2:\n",
    "            return max(lst)\n",
    "        else:\n",
    "            return lst[len(lst)-3]"
   ]
  }
