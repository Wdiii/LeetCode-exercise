   "source": [
    "class Solution:\n",
    "    def maxSubArray(self, nums: List[int]) -> int:\n",
    "        c=nums[0]\n",
    "        maxsum=nums[0]\n",
    "        for i in range (1, len(nums)):\n",
    "            c=max(c+nums[i], nums[i])\n",
    "            maxsum=max(maxsum, c)\n",
    "        return maxsum"
   ]
  }
