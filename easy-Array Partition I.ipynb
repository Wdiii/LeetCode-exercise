   "source": [
    "class Solution:\n",
    "    def arrayPairSum(self, nums: List[int]) -> int:\n",
    "        nums.sort()\n",
    "        n=len(nums)\n",
    "        sum=0\n",
    "        i=0\n",
    "        while i<n:\n",
    "            sum+=nums[i]\n",
    "            i+=2\n",
    "        return sum"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "return sum(sorted(nums)[::2])"
   ]
  }
