    "class Solution:\n",
    "    def rotate(self, nums: List[int], k: int) -> None:\n",
    "        \"\"\"\n",
    "        Do not return anything, modify nums in-place instead.\n",
    "        \"\"\"\n",
    "        n=len(nums)\n",
    "        mid=n-k\n",
    "        tmp = nums[:mid] \n",
    "        nums[:k] = nums[mid:]\n",
    "        nums[k:] =tmp"
   ]
  }
